{
    "name": "jorrit/sonata-clone-action-bundle",
    "description": "Sonata Admin extension that adds a clone action.",
    "keywords": [
        "sonata",
        "admin",
        "extension",
        "clone"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jorrit Schippers",
            "email": "jorrit@ncode.nl"
        }
    ],
    "require": {
        "php": "^8.0",
        "sonata-project/admin-bundle": "^4.0",
        "symfony/framework-bundle": "^5.0 || ^6.0",
        "symfony/http-kernel": "^5.1.5 || ^6.0",
        "symfony/property-access": "^5.0 || ^6.0",
        "symfony/property-info": "^5.0 || ^6.0"
    },
    "require-dev": {
        "doctrine/orm": "^3.3",
        "gedmo/doctrine-extensions": "^3.0",
        "sonata-project/translation-bundle": "^3.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Jorrit\\SonataCloneActionBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jorrit\\SonataCloneActionBundle\\Tests\\": "tests/"
        }
    }
}
